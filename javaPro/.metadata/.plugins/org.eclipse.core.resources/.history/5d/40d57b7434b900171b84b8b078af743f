<script type="text/javascript">
$("#Button1").click(function(){
	System.out.println("hahah");
	$.ajax({
		url:"/htmlGuiSimple/rest/hello/addbyname",
		type:"POST",
		data:{'name':"hahaha"},
		success:function(result){
			alert(result);
		},
		error:function(){
			alert("add category failed!");
		}
	});
});
